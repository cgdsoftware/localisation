{
    "name": "laravel-enso/localisation",
    "description": "Localisation management dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "localisation"
    ],
    "homepage": "https://github.com/laravel-enso/localisation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "jenssegers/date": "^3.4",
        "laravel-enso/core": "4.8.*",
        "laravel-enso/forms": "3.4.*",
        "laravel-enso/helpers": "1.15.*",
        "laravel-enso/select": "3.3.*",
        "laravel-enso/migrator": "1.1.*",
        "laravel-enso/tables": "3.2.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Localisation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Localisation\\AppServiceProvider",
                "LaravelEnso\\Localisation\\AuthServiceProvider"
            ],
            "aliases": {}
        }
    }
}